						<hr />
					</XHTML>
				</COL>
			</ROW>
			<ROW>
			</ROW>
		</HEADER>
		<FOOTER>
<!-- PIE DE PAGINA -->
<ROW><COL CELLCLASS='GHC' COLSPAN='11' TYPE='RAW_EXPRESSION'>getRawFooter(true)</COL></ROW>
